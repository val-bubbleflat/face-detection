{
  "name": "Face Detector by Val",
  "version": "1.1",
  "description": "Detect all faces",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["js/app.js"],
      "css": ["css/style.css"]
    }
  ]
}